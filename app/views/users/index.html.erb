<p id="notice"><%= notice %></p>

<h1>Admins</h1>

<table>
  <thead>
    <tr>
      <th class="user_index">Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="user_index"><%= user.name %></td>
        <td class="user_index"><%= link_to 'Show', user %></td>
        <td class="user_index"><%= link_to 'Edit', edit_user_path(user) %></td>
        <td class="user_index"><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

